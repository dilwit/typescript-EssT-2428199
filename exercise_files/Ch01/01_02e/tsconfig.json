{
    "compilerOptions": {
        "outDir": "build", /* Setting output .js dir, this will work if only noEmit is set to true*/
        "target": "esnext", /*ES3, ES6 (most browsers support this) are other values you can set here. esnext => always use the latest version like ES6 */
        "noEmit": true
    },
    "include": ["src/**/*"]
}
